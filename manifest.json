{
  "manifest_version": 3,
  "name": "Netflix popup remover",
  "version": "1.0",
  "description": "Removes irritating popup",
  "content_scripts": [
    {
      "matches": [
        "*://*.netflix.com/*",
        "*://escapefromtarkov.fandom.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}